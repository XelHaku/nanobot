{
  "agents": {
    "defaults": {
      "workspace": "./coordinator-workspace",
      "model": "openrouter/anthropic/claude-sonnet-4",
      "maxTokens": 8192,
      "temperature": 0.1,
      "maxToolIterations": 40,
      "memoryWindow": 50
    }
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "YOUR_TELEGRAM_BOT_TOKEN_HERE",
      "allowFrom": ["YOUR_TELEGRAM_USER_ID_HERE"]
    },
    "discord": {
      "enabled": false,
      "token": "YOUR_DISCORD_BOT_TOKEN_HERE",
      "allowFrom": ["YOUR_DISCORD_USER_ID_HERE"],
      "groupPolicy": "mention",
      "roleIds": [],
      "siblingBots": {}
    },
    "sendProgress": true,
    "sendToolHints": false,
    "sendToolResults": false
  },
  "providers": {
    "openrouter": {
      "apiKey": "YOUR_OPENROUTER_API_KEY_HERE"
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18792
  },
  "heartbeat": {
    "enabled": false,
    "intervalSeconds": 1800
  },
  "tools": {
    "web": {
      "search": {
        "apiKey": "YOUR_BRAVE_SEARCH_API_KEY_HERE",
        "maxResults": 5
      }
    },
    "exec": {
      "timeout": 120
    },
    "restrictToWorkspace": false
  }
}
